                                                                <script type="text/javascript">
                        function setTabDetails(tabName){
                            var emailAddr = $("#mail_" + tabName).val();
                            var passW = $("#password_" + tabName).val();
                            var loginType = 'email';

                            if(tabName=='universe-professionals' || tabName=='universe-companies'){
                                loginType = 'screen';
                            }
                            if(!validateLoginForm(emailAddr, passW, loginType)){
                                setLogin(emailAddr, passW);
                            }
                        }

                        function setLogin(login,password){
                            $("#_58_login").val(login);
                            $("#_58_password").val(password);
                            $("#_58_fm").submit();
                        }

                        function validateLoginForm(login,password,type){
                            var invalidForm = false;
                            if(login=='' || password==''){
                                invalidForm = true;
                            }
                            if(type=='email'){
                                if(!validateLoginEmail(login)){
                                    invalidForm = true;
                                    $(".login-error_" + $('#currentTabMobility').val()).html("Veuillez saisir une adresse email valide.");
                                    $(".login-error_" + $('#currentTabMobility').val()).show();
                                }
                            } 	else if(login.indexOf("@")!=-1){
                                invalidForm = true;
                                $(".login-error_" + $('#currentTabMobility').val()).html("Veuillez saisir un code partenaire valide.");
                                $(".login-error_" + $('#currentTabMobility').val()).show();
                            }
                            return invalidForm;
                        }

                        function setForgotDetails(tabName){
                            var emailAddr = $("#emailAddress_" + tabName).val();
                            var loginType = 'email';

                            if(tabName=='universe-professionals' || tabName=='universe-companies'){
                                loginType = 'screen';
                            }
                            if(!validateForgotForm(emailAddr,loginType)){
                                setForgot(emailAddr);
                            }
                        }

                        function setForgot(login){
                            var newFormAction = $("#_58_fm").attr("action");
                            newFormAction = newFormAction + "&_58_currentTabMobility=" + $('#currentTabMobility').val();

                            $("#_58_emailAddress").val(login);
                            $("#_58_fm").attr("action",newFormAction);
                            $("#_58_fm").submit();
                        }

                        function validateForgotForm(login,type){
                            var invalidForm = false;
                            if(login==''){
                                invalidForm = true;
                            }
                            if(type=='email'){
                                if(!validateLoginEmail(login)){
                                    invalidForm = true;
                                    $(".login-error_" + $('#currentTabMobility').val()).html("Veuillez saisir une adresse email valide.");
                                    $(".login-error_" + $('#currentTabMobility').val()).show();
                                }
                            } else {
                                if(validateLoginEmail(login)){
                                    invalidForm = true;
                                    $(".login-error_" + $('#currentTabMobility').val()).html("Veuillez saisir un code partenaire valide.");
                                    $(".login-error_" + $('#currentTabMobility').val()).show();
                                }
                            }

                            return invalidForm;
                        }

                        function validateLoginEmail(mail) {
                            var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
                            if(!emailReg.test(mail)) {
                                return false;
                            } else {
                                return true;
                            }
                        }

                        function closeLogin(){
                            $(".layer--login").hide();
                        }

                        function setForgotURL(){
                            var forgotPwdLink = $("#forgotPasswordLink").attr('href') + "&_58_currentTabMobility=" + $('#currentTabMobility').val();
                            if($('#currentTabMobility').val()=='universe-professionals'){
                                forgotPwdLink = forgotPwdLink + '&isEPSS=Y';
                            }
                            location.href = forgotPwdLink;
                        }

                        function submitFormLogin(e, tabCode) {
                            if(e.keyCode == 13) {
                                setTabDetails(tabCode);
                            }
                        }

                        function submitFormForgotPass(e, tabCode) {
                            if(e.keyCode == 13) {
                                setForgotDetails(tabCode);
                            }
                        }

                        $( document ).ready(function() {
                            var currentTab = $("#currentTabMobility").val();
                            var isIOS = /(iPad|iPhone)/g.test( navigator.userAgent );
                            var isAndroid = navigator.userAgent.toLowerCase().indexOf("android") > -1;

                            $(".device-only--android .andro").attr("src",$("#imagePath").val() + "btn-dwl-googleplay.png");
                            $(".device-only--ios .ios").attr("src",$("#imagePath").val() + "btn-dwl-appstore.png");

                            // Mobile detection
                            if(isIOS || isAndroid){
                                $( ".universe-consumers .hidden-xs" ).css("display","none !important");
                                $( ".universe-businesses .hidden-xs" ).css("display","none !important");

                                $( ".universe-consumers .visible-xs" ).css("display","none !important");
                                $( ".universe-businesses .visible-xs" ).css("display","none !important");
                            }
                            if(isIOS){
                                $(".device-only--android").hide();
                            }
                            if(isAndroid){
                                $(".device-only--ios").hide();
                            }
                            $(".nav-pills li:last-child").click(function() {
                                if($('.forgot-pwd-box [name="_58_formDate"]').val()!=undefined){
                                    location.href = location.href.split("?")[0];
                                    return false;
                                }
                                if($('.login-box [name="_58_formDate"]').val()!=undefined){
                                    if( $(".login-portlet-hide .portlet-msg-success").html()!=undefined){
                                        if($(".login-portlet-hide .portlet-msg-success").html().length>0){
                                            location.href = location.href.split("?")[0];
                                            return false;
                                        }
                                    }
                                }
                            });

                            $('#PART_ANDRO_universe-consumers').attr($("#partAndroidId").val());
                            $('#PART_IOS_universe-consumers').attr($("#partAppleId").val());
                            $('#PART_ANDRO_universe-businesses').attr($("#partAndroidId").val());
                            $('#PART_IOS_universe-businesses').attr($("#partAppleId").val());

                            $('#PRO_ANDRO').attr($("#proAndroidId").val());
                            $('#PRO_IOS').attr($("#proAppleId").val());

                            $(".universe-consumers .form-control").val('');
                            $(".universe-businesses .form-control").val('');
                            $(".universe-companies .form-control").val('');
                            $(".universe-professionals .form-control").val('');

                            $( ".universe-consumers" ).click(function() {
                                $('#currentTabMobility').val('universe-consumers');
                                $('#USERTYPE').val('PARTICULIER');
                            });
                            $( ".universe-businesses" ).click(function() {
                                $('#currentTabMobility').val('universe-businesses');
                                $('#USERTYPE').val('PARTICULIER');
                            });

                            $( ".universe-companies" ).click(function() {
                                $('#currentTabMobility').val('universe-companies');
                                $('#labelEmail_universe-companies').html("Votre identifiant");
                                $('#labelEmail1_universe-companies').html("Votre identifiant");
                                $('#labelPassword_universe-companies').html("Votre mot de passe");
                                $('#labelEmail_mob_universe-companies').html("Votre identifiant");
                                $('#labelEmail1_mob_universe-companies').html("Votre identifiant");
                                $('#labelPassword_mob_universe-companies').html("Votre mot de passe");
                                $('#USERTYPE').val('CORRESPONDANT');
                            });
                            $( ".universe-professionals" ).click(function() {
                                $('#currentTabMobility').val('universe-professionals');
                                $('#labelEmail_universe-professionals').html("Identifiant professionnel");
                                $('#labelEmail1_universe-professionals').html("Identifiant professionnel");
                                $('#labelPassword_universe-professionals').html("Votre mot de passe");
                                $('#labelEmail_mob_universe-professionals').html("Identifiant professionnel");
                                $('#labelEmail1_mob_universe-professionals').html("Identifiant professionnel");
                                $('#labelPassword_mob_universe-professionals').html("Votre mot de passe");
                                $('#USERTYPE').val('EPS');
                            });

                            //Set contact link
                            //var contactLink = $("#CONTACT_TECHNIQUE_HELP_FORM_LINK").attr('href');
                            //$('.login-contact-form').attr('href',contactLink);

                            if($('#loginPopUpShow').val() == "true"){
                                if($("#login-accordion .panel").html()!=undefined){
                                    $(".layer--login").show();
                                } else {
                                    showLoginPopUp();
                                }

                                // Set error message
                                var error_msg_login = $(".memberlogin_container .portlet-msg-error").html();

                                $(".login-error_" + currentTab).html(error_msg_login);
                                $(".login-error_" + currentTab).show();
                                // Open tab
                                $("." + currentTab + " h4.panel-title a").trigger("click");
                                //Set value for login field
                                $("#mail_" + currentTab).val($("#_58_login").val());
                                $("#password_" + currentTab).val($("#_58_password").val());
                            }

                            if($('.forgot-pwd-box [name="_58_formDate"]').val()!=undefined){
                                if($(".forgot-pwd-box .portlet-msg-error").html()!=undefined){
                                    if($(".forgot-pwd-box .portlet-msg-error").html().length>0){
                                        // Set error message
                                        var error_msg_login = $(".forgot-pwd-box .portlet-msg-error").html();

                                        $(".login-error_" + currentTab).html(error_msg_login);
                                        $(".login-error_" + currentTab).show();

                                        //Set value for login field
                                        $("#emailAddress_" + currentTab).val($("#_58_emailAddress").val())
                                    }
                                }

                                // Open tab
                                if($("#login-accordion .panel").html()!=undefined){
                                    $(".layer--login").show();
                                } else {
                                    showLoginPopUp();
                                }
                                //$("." + currentTab + " .forgotten-password--form-login").hide();
                                //$("." + currentTab + " .forgotten-password--form-retrieve").show();
                                $(".forgotten-password--form-login").hide();
                                $(".forgotten-password--form-retrieve").show();
                                $("." + currentTab + " h4.panel-title a").trigger("click");
                            }
                            $( window ).load(function() {
                                if($('.login-box [name="_58_formDate"]').val()!=undefined){
                                    if( $(".login-portlet-hide .portlet-msg-success").html()!=undefined){
                                        if($(".login-portlet-hide .portlet-msg-success").html().length>0){
                                            // Set Success message
                                            var forgot_success_msg = $(".login-portlet-hide .portlet-msg-success").html();
                                            if($("#login-accordion .panel").html()!=undefined){
                                                $(".layer--login").show();
                                            } else {
                                                showLoginPopUp();
                                            }
                                            $(".login-container").hide();
                                            $("#success-forgot").html(forgot_success_msg);
                                            $("#success-forgot").show();
                                        }
                                    }
                                }
                            });
                        });
                    </script>
